<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ming.car.mapper.RulesMapper">
    <select id="getRuleNameByWarnId" resultType="com.ming.car.entity.dto.Rules">
         select * from rules where battery_type = #{batteryType} and rule_id = #{warnId}
    </select>
    <select id="getRuleNameByBatteryType" resultType="com.ming.car.entity.dto.Rules">
          select * from rules where battery_type = #{batteryType}
    </select>
</mapper>
